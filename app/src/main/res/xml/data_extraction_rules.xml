<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude API keys and sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="api_keys.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude API keys from device transfer -->
        <exclude domain="sharedpref" path="api_keys.xml"/>
    </device-transfer>
</data-extraction-rules>

